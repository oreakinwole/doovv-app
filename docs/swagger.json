{
  "openapi": "3.0.0",
  "paths": {
    "/auth/login": {
      "post": {
        "operationId": "AuthController_login",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login successful"
          },
          "401": {
            "description": "Invalid credentials"
          }
        },
        "tags": [
          "auth"
        ]
      }
    },
    "/bookings": {
      "post": {
        "operationId": "BookingsController_create",
        "parameters": [
          {
            "name": "idempotency-key",
            "required": true,
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Idempotency-Key",
            "in": "header",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateBookingDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Booking created successfully"
          }
        },
        "tags": [
          "bookings"
        ],
        "security": [
          {
            "bearer": []
          }
        ]
      },
      "get": {
        "operationId": "BookingsController_findAll",
        "parameters": [],
        "responses": {
          "200": {
            "description": "List of bookings"
          }
        },
        "tags": [
          "bookings"
        ],
        "security": [
          {
            "bearer": []
          }
        ]
      }
    },
    "/bookings/{id}/status": {
      "patch": {
        "operationId": "BookingsController_updateStatus",
        "parameters": [
          {
            "name": "id",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateBookingStatusDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Booking status updated"
          }
        },
        "tags": [
          "bookings"
        ],
        "security": [
          {
            "bearer": []
          }
        ]
      }
    },
    "/bookings/history": {
      "get": {
        "operationId": "BookingsController_findHistory",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Booking history with legacy data"
          }
        },
        "tags": [
          "bookings"
        ],
        "security": [
          {
            "bearer": []
          }
        ]
      }
    }
  },
  "info": {
    "title": "Doovo Car Wash API",
    "description": "Car wash booking and management system",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@doovo.com"
          },
          "password": {
            "type": "string",
            "example": "password123"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "CreateBookingDto": {
        "type": "object",
        "properties": {
          "serviceType": {
            "type": "string",
            "enum": [
              "DROP_OFF",
              "MOBILE",
              "PICKUP_RETURN"
            ]
          },
          "vehicleInfo": {
            "type": "string",
            "example": "2024 Honda Civic - Blue"
          },
          "address": {
            "type": "string",
            "example": "123 Main St, City"
          },
          "scheduledAt": {
            "type": "string",
            "example": "2024-12-25T10:00:00Z"
          },
          "notes": {
            "type": "string",
            "example": "Please be gentle with the paint"
          }
        },
        "required": [
          "serviceType",
          "vehicleInfo",
          "address",
          "scheduledAt",
          "notes"
        ]
      },
      "UpdateBookingStatusDto": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "PENDING",
              "CONFIRMED",
              "IN_PROGRESS",
              "COMPLETED",
              "CANCELLED"
            ]
          },
          "notes": {
            "type": "string",
            "example": "Work completed successfully"
          }
        },
        "required": [
          "status",
          "notes"
        ]
      }
    }
  }
}